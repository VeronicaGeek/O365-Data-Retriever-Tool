                    <TextBlock x:Name="NbrOfDomainsTextBlock" HorizontalAlignment="Left" Margin="96,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Bold" Height="18" Width="64"/>
                    <Label x:Name="PlansLabel" Content="Plans:" HorizontalAlignment="Left" Margin="390.8,27,0,0" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="29" Width="45"/>
                    <Label x:Name="GlobalAdminsLabel" Content="Global Admins:" HorizontalAlignment="Left" Margin="391,340,0,0" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="28" Width="104"/>
                    <Button x:Name="ExportDomainsButton" Content="Export To CSV" HorizontalAlignment="Left" Margin="215,643,0,0" VerticalAlignment="Top" Width="121" Height="31" FontWeight="Normal" Background="#FFACABAB" Grid.Column="1">
                        <Button.Resources>
                            <Style TargetType="{x:Type Border}">
                                <Setter Property="CornerRadius" Value="7"/>
                            </Style>
                        </Button.Resources>
                    </Button>
                    <Button x:Name="ExportGAButton" Content="Export To CSV" HorizontalAlignment="Left" Margin="761,643,0,0" VerticalAlignment="Top" Width="121" Height="31" FontWeight="Normal" Background="#FFACABAB" Grid.Column="1">
                        <Button.Resources>
                            <Style TargetType="{x:Type Border}">
                                <Setter Property="CornerRadius" Value="7"/>
                            </Style>
                        </Button.Resources>
                    </Button>
                    <Button x:Name="ExportPlansButton" Content="Export To CSV" HorizontalAlignment="Left" Margin="761,293,0,0" VerticalAlignment="Top" Width="121" Height="31" FontWeight="Normal" Background="#FFACABAB" Grid.Column="1">
                        <Button.Resources>
                            <Style TargetType="{x:Type Border}">
                                <Setter Property="CornerRadius" Value="7"/>
                            </Style>
                        </Button.Resources>
                    </Button>
                    <!-- At a Glance Section-->
                    <Label x:Name="NbrOfUsersLabel" Content="Total Nbr of Users:" HorizontalAlignment="Left" Margin="971,127,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="29" Width="128"/>
                    <Label x:Name="NbrOfContactsLabel" Content="Total Nbr of Contacts:" HorizontalAlignment="Left" Margin="971,337,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="29" Width="148"/>
                    <Label x:Name="NbrOfGuestsLabel" Content="Total Nbr of Guests:" HorizontalAlignment="Left" Margin="971,370,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="29" Width="136"/>
                    <Label x:Name="NbrOfGroupsLabel" Content="Total Nbr of Groups:" HorizontalAlignment="Left" Margin="971,404,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="29" Width="139"/>
                    <Label x:Name="NbrOfSharedMlxLabel" Content="Total Nbr of Shared Mailboxes:" HorizontalAlignment="Left" Margin="971,438,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="28" Width="204"/>
                    <Label x:Name="NbrOfRoomsLabel" Content="Total Nbr of Rooms:" HorizontalAlignment="Left" Margin="971,472,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="29" Width="136"/>
                    <Label x:Name="NbrOfEquipmentLabel" Content="Total Nbr of Equipement:" HorizontalAlignment="Left" Margin="971,506,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="29" Width="169"/>
                    <TextBlock x:Name="NbrOfPlansTextBlock" HorizontalAlignment="Left" Margin="436,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="1" Height="18" Width="64" FontWeight="Bold"/>
                    <TextBlock x:Name="NbrOfGATextBlock" HorizontalAlignment="Left" Margin="494,345,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column="1" Height="18" Width="63" FontWeight="Bold"/>
                    <TextBlock x:Name="NbrOfShdMlbxTtextBlock" HorizontalAlignment="Left" Margin="1181,443,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="18" Width="23"/>
                    <TextBlock x:Name="NbrOfEquipTextBlock" HorizontalAlignment="Left" Margin="1181,511,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="18" Width="23"/>
                    <TextBlock x:Name="NbrofRoomsTextBlock" HorizontalAlignment="Left" Margin="1181,477,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="19" Width="23"/>
                    <TextBlock x:Name="NbrOfGroupsTextBlock" HorizontalAlignment="Left" Margin="1181,409,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="19" Width="23"/>
                    <TextBlock x:Name="NbrOfGuestsTextBlock" HorizontalAlignment="Left" Margin="1181,375,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="18" Width="23"/>
                    <TextBlock x:Name="NbrOfContactsTextBlock" HorizontalAlignment="Left" Margin="1181,342,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="19" Width="23"/>
                    <TextBlock x:Name="NbrOfUsersTextBlock" HorizontalAlignment="Left" Margin="1181,132,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="18" Width="23"/>
                    <Label x:Name="NbrOfSyncedUsersLabel" Content="Total Nbr of Synced Users:" HorizontalAlignment="Left" Margin="971,158,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="29" Width="176"/>
