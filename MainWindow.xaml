                <Grid Background="#eff0f2" Margin="-4,-3,-2.4,-2.6" >
                    <TabControl x:Name="tabControl1" HorizontalAlignment="Left" Height="712" VerticalAlignment="Top" Width="1541" Margin="0,0,0,-0.4">
