                        <ColumnDefinition Width="1261*"/>
                    </Grid.ColumnDefinitions>
                    <DataGrid x:Name="DomainsDataGrid" HorizontalAlignment="Left" Margin="29,61,0,0" VerticalAlignment="Top" Height="567" Width="502" AutoGenerateColumns="False" AlternatingRowBackground="#d0edd2" AlternationCount="2" IsReadOnly="True" CanUserSortColumns="True" CanUserAddRows="False" Grid.ColumnSpan="2">
                        <DataGrid.Columns>
                            <DataGridTextColumn Width="auto" Header="Name" Binding="{Binding Name}">
                                <DataGridTextColumn.ElementStyle>
                                    <Style>
                                        <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="auto" Header="Default" Binding="{Binding IsDefault}">
                                <DataGridTextColumn.ElementStyle>
                                    <Style>
                                        <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="auto" Header="Status" Binding="{Binding Status}">
                                <DataGridTextColumn.ElementStyle>
                                    <Style>
                                        <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="auto" Header="Authentication" Binding="{Binding Authentication}">
                                <DataGridTextColumn.ElementStyle>
                                    <Style>
