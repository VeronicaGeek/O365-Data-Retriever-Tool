                    <TextBlock x:Name="NbrOfSyncedUsersTextBlock" HorizontalAlignment="Left" Margin="1181,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="18" Width="23"/>
                    <TextBlock x:Name="NbrOfCloudUsersTextBlock" HorizontalAlignment="Left" Margin="1181,194,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="19" Width="23"/>
                    <Label x:Name="NbrOfBlockedUsersLabel" Content="Total Nbr of Blocked Users:" HorizontalAlignment="Left" Margin="971,223,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Grid.Column="1" Height="29" Width="180"/>
                    <Label x:Name="BlockedNotLicensedLabel" Content="(Not licensed)" HorizontalAlignment="Left" Margin="971,242,0,0" VerticalAlignment="Top" FontStyle="Italic" Grid.Column="1" Height="29" Width="92" FontFamily="Myriad Pro"/>
                    <TextBlock x:Name="NbrOfBlockedUsersTextBlock" HorizontalAlignment="Left" Margin="1181,227,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Grid.Column="1" Height="18" Width="23"/>
                    <Label x:Name="NbrOfBlockedAndLicensedUsersLabel" Content="Total Nbr of Blocked &#xD;&#xA;And Licensed Users:" HorizontalAlignment="Left" Margin="971,278,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Myriad Pro" Width="181" Height="50" FontStyle="Italic" Grid.Column="1"/>
                    <TextBlock x:Name="NbrOfBlockedAndLicensedUsersTextBlock" HorizontalAlignment="Left" Margin="1181,292,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Bold" Grid.Column="1" Height="18" Width="25"/>
                    <TextBlock x:Name="AtAGlanceTextBlock" Grid.Column="1" HorizontalAlignment="Left" Margin="942,94,0,0" TextWrapping="Wrap" Text="At a Glance:" VerticalAlignment="Top" FontWeight="Bold" TextDecorations="Underline" FontStyle="Italic" FontSize="18" Foreground="#FFAA4049"/>
