        <TextBlock x:Name="ContactPhoneNbrTextBlock" HorizontalAlignment="Left" Margin="329,124,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="13"/>
        <TextBlock x:Name="OrgNameTextBlock" HorizontalAlignment="Left" Margin="329,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="13"/>
        <TextBlock x:Name="CountryTextBlock" HorizontalAlignment="Left" Margin="329,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="13"/>
        <TextBlock x:Name="TechContactTextBlock" HorizontalAlignment="Left" Margin="329,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="13"/>
